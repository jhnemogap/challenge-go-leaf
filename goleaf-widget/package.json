{
  "name": "goleaf-widget",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
		"install:0": "npm run clean:full && npm install",
    "dev": "vite",
    "build": "vite build --outDir build",
    "preview": "vite preview --outDir build",
		"clean": "rm -rf build/ dist/",
		"clean:full": "npm run clean && rm -rf node_modules/ package-lock.json",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "2.4.2",
    "@tsconfig/svelte": "5.0.0",
    "svelte": "4.0.3",
    "svelte-check": "3.4.4",
    "tslib": "2.6.0",
    "typescript": "5.0.2",
    "vite": "4.4.0"
  }
}
